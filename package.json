{
    "name": "radnight",
    "version": "0.1.0",
    "description": "Dark mode for Statamic v3 Control Panel",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andymnewhouse/radnight.git"
    },
    "keywords": [
        "statamic-v3",
        "statamic",
        "dark-mode"
    ],
